    "def replace_in_columns_names(df, from_symbol, to_symbol):\n",
    "    names = {}\n",
    "    \n",
    "    for name in df.columns:\n",
    "        if from_symbol not in name:\n",
    "            continue\n",
    "        \n",
    "        names[name] = name.replace(from_symbol, to_symbol)\n",
    "        \n",
    "    return df.rename(names, axis=1)"
   ]
